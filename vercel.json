{
  "version": 2,
  "name": "ykproject",
  "builds": [
      { "use": "@vercel/static-build", "src": "api/package.json"}
  ],
  "routes": [
      {
          "src": "(.*).js",
          "dest": "/$1.js"            
      },
      {
          "src": "(.*).json",
          "dest": "/$1.json"            
      },
      {
          "src": "/.*",
          "dest": "web/index.html"
      }
  ]
} 
